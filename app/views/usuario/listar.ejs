<html lang="en">
<head>
  <title>Trabalho 3</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https:maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https:ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https:maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>

<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">Trabalho 3</a>
    </div>
    <ul class="nav navbar-nav">
	<% for(var i in principal) {
              var link = principal[i];
          %>
      <li class="active"><a href="<%=link.link%>"><%=link.rota%></a></li>
	  <% } %>

    </ul>
  </div>
</nav>

<div class="container">
  <h1>Lista de Usu√°rios</h1>
    <table>
      <thead>
        <tr>
          <th>Nome</th>
          <th>E-Mail</th>
        </tr>
      </thead>
      <tbody>
        <%
        for (var i in usuarios){
          var usuario = usuarios[i];
        %>
        <tr>
          <td><a href="/detalhes-usuario.html?id=<%=usuario._id%>"><%=usuario.nome%></a></td>
          <td><%=usuario.email%></td>
        </tr>
        <% } %>
      </tbody>
    </table>
</div>

</body>
</html>
